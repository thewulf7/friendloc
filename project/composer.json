{
    "name"              : "thewulf7/friendloc",
    "description"       : "Friend locator test case",
    "authors"           : [
        {
            "name"  : "Evgenii Utkin",
            "email" : "thewulf7@gmail.com"
        }
    ],
    "config"            : {
        "bin-dir" : "bin"
    },
    "autoload"          : {
        "psr-4" : {
            "thewulf7\\friendloc\\" : "src/"
        }
    },
    "require"           : {
        "php"                          : ">=7.0",
        "doctrine/orm"                 : "2.5.2",
        "symfony/yaml"                 : "2.*",
        "doctrine/migrations"          : "1.2.*",
        "creof/doctrine2-spatial"      : "1.0.0",
        "php-di/php-di"                : "*",
        "twig/twig"                    : "^1.18",
        "elasticsearch/elasticsearch"  : "*",
        "egeloen/google-map"           : "dev-master",
        "willdurand/geocoder"          : "*",
        "widop/http-adapter"           : "1.0.*",
        "swiftmailer/swiftmailer"      : "@stable",
        "fxp/composer-asset-plugin"    : "~1.1",
        "bower-asset/bootstrap"        : "3.3.6",
        "bower-asset/requirejs"        : "*",
        "bower-asset/requirejs-twig"   : "*",
        "bower-asset/jquery-typewatch" : "*"
    },
    "require-dev"       : {
        "phpunit/phpunit"     : "5.0.*",
        "phpunit/php-invoker" : ">=1.1.0@stable"
    },
    "extra"             : {
        "asset-installer-paths" : {
            "bower-asset-library" : "src/web/assets/vendor"
        }
    },
    "license"           : "MIT",
    "minimum-stability" : "dev"
}